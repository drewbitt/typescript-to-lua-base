{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "types": ["@typescript-to-lua/language-extensions"],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
  },
  "tstl": {
    "luaTarget": "5.3",
    "noImplicitSelf": true,
  }
}